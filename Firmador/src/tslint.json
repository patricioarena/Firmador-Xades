{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    // "directive-selector": [true, "attribute", "app", "camelCase"],
    // "component-selector": [true, "element", "app", "kebab-case"],
    "max-line-length": [
      true,
      {
        "limit": 140,
        "ignore-pattern": "^import |^export {(.*?)}",
        "check-strings": true,
        "check-regex": true
      }
    ]
  }
}

